<script type="text/javascript">
//刷新验证码
$('#vcode').click(function () {
    var src = $(this).attr('src')+'?v='+Math.random();
    $(this).attr('src',src);
});
$('#tcode').click(function () {
    var src = $(this).attr('src')+'?t='+Math.random();
    $(this).attr('src',src);
});
</script>