            </div>
            <form action="{:url('home/Log/inserts')}" method="post">
                <div class="form-input">
                    <label for="username">请输入用户名</label>
                    <input type="text" id="username" name="front_name" />
                </div>
                <div class="form-input">
                    <label for="password">请输入密码</label>
                    <input type="password" id="password" name="front_password" />
                </div>
                
                <div class="login_box" style="margin: 0 auto;width: 200px;">
                	<input type="text" name="vcode" class="form-control" placeholder="验证码" aria-describedby="sizing-addon1"/>
            	</div>

            	<div class="col">
                	<img src="{:captcha_src()}" id="vcode">
                </div>
                <div class="form-input">
                    <button>登录</button>
                </div>
            </form>
        </div>
    </div>

    <div class="model-box-2">
        <div class="contents">
            <div class="titles">
                <span>注册</span>
                <i class="closes">×</i>
            </div>
            <form action="{:url('home/Log/register')}" method="post">
